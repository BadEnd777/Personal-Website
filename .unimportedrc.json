{
    "entry": ["src/**/*.{ts,tsx,js,jsx}"],
    "extensions": [".ts", ".tsx"],
    "ignorePatterns": ["**/node_modules/**"],
    "ignoreUnused": [
        "@iconify-json/simple-icons",
        "@iconify-json/tabler",
        "@iconify/tailwind",
        "rehype-stringify",
        "remark-parse",
        "remark-rehype",
        "tailwindcss-animate",
        "unified"
    ],
    "ignoreUnresolved": [
        "rehype-stringify",
        ["rehype-stringify", ["src\\lib\\blog.ts"]],
        "remark-parse",
        ["remark-parse", ["src\\lib\\blog.ts"]],
        "remark-rehype",
        ["remark-rehype", ["src\\lib\\blog.ts"]],
        "unified",
        ["unified", ["src\\lib\\blog.ts"]]
    ],
    "respectGitignore": true,
    "ignoreUnimported": []
}
