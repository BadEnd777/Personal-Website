    "devDependencies": {
        "@types/node": "^20.11.16",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "autoprefixer": "^10.4.17",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-solid": "^0.13.1",
        "husky": "^9.0.10",
        "lint-staged": "^15.2.2",
        "postcss": "^8.4.35",
